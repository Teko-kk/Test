function main() {
while (frontIsClear()) {
   
   Step1();
   Step2();
}
   
}


function Step1() {
      move();
      putBeeper();
      turnLeft();
      move();
      putBeeper();
      move();
      putBeeper();
      turnRight();
   }
      
function Step2() {
   if (beepersPresent()) {
      turnRight();
      move();
      move();
      turnLeft();
      move();
   }
}
